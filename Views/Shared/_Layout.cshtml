<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - clienteApi</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/estilos_personalizados.css" />

    <!-- Font Awesome Icons -->
    <link rel="stylesheet" href="~/fontawesome-free/css/all.min.css">

    <!-- Theme style -->
    <link rel="stylesheet" href="~/css/adminlte.min.css">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/dataTables.bootstrap4.min.css">
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand menu enlace" asp-area="" asp-controller="Dashboard" asp-action="Index" data-url="@Url.Action("DashboardLink", "Dashboard")">clienteApi</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark menu enlace" data-url="@Url.Action("Index", "Home")" >Clientes</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark menu enlace" data-url="@Url.Action("Index","SerServicios")" >Servicios</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container-fluid" id="div_menu_principal">
        @*<main role="main" class="pb-3">*@
            @RenderBody()
        @*</main>*@
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - clienteApi - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>

    <!-- Bootstrap -->
    <script src="~/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/js/adminlte.min.js"></script>

   
    <script src="~/lib/bootstrap/dist/js/jquery.dataTables.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/dataTables.bootstrap4.min.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    @*<script src="~/js/dashboard2.js"></script>*@
    @{
        await RenderSectionAsync("Scripts", required: false);

        await Html.RenderPartialAsync("~/Views/Shared/_ScriptMenu.cshtml");
    }
</body>
</html>
